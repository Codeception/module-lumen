{
    "name": "codeception/module-lumen",
    "description": "Codeception module for Lumen framework",
    "keywords": ["codeception", "lumen"],
    "homepage": "https://codeception.com/",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name":"Jan-Henk Gerritsen"
        },
        {
            "name": "Gustavo Nieves",
            "homepage": "https://medium.com/@ganieves"
        }
    ],
    "minimum-stability": "RC",
    "require": {
        "php": "^7.4 | ^8.0",
        "ext-json": "*",
        "codeception/lib-innerbrowser": "^2.0",
        "codeception/codeception": "^4.1"
    },
    "require-dev": {
        "codeception/module-asserts": "^2.0",
        "laravel/lumen-framework": "^6.3 | ^7.2 | ^8.3",
        "vlucas/phpdotenv": "^3.6 | ^4.1 | ^5.2"
    },
    "autoload":{
        "classmap": ["src/"]
    },
    "config": {
        "classmap-authoritative": true
    }
}
